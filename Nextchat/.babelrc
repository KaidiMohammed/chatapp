{
    "presets": [
        [
            "next/babel"
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-optional-chaining",
        "@babel/plugin-proposal-nullish-coalescing-operator",
        "dynamic-import-node",
        [
            "module-resolver",
            {
                "alias": {
                    "@services": "./services",
                    "@stateManager": "./stateManager",
                    "@components": "./components",
                    "@config": "./config"
                }
            }
        ]
    ]
}